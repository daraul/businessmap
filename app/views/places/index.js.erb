var content = ''

<% @places.each do |place| %>
    content += '<%= j content_tag(:div, class: "place listing container", id: "place_#{place.id}", data: { price: place.price, rooms: place.rooms, bathrooms: place.bathrooms, for: place.for } ) { render place } %>'
<% end %>

$('#listings.container').append(content);

<% if @places.next_page %>
    $('#infiniteScrolling').html('<%= j will_paginate @places %>');
<% else %>
    $(window).off('scroll');
    
    $('.pagination').remove();
<% end %>