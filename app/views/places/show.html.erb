<%= javascript_tag do %>
    window.image_urls = []
    
    <% @place.pictures.each do |picture| %>
        window.image_urls.push("<%= picture.image.url %>")
    <% end %>
    
    window.image_thumb_urls = []
    
    <% @place.pictures.each do |picture| %>
        window.image_thumb_urls.push("<%= picture.image.url(:thumb) %>")
    <% end %>
<% end %>